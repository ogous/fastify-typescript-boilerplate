{
  "name": "fastify-typescript-boilerplate",
  "packageManager": "npm@8.19.2",
  "license": "MIT",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch './src' --exec node --loader ts-node/esm --experimental-specifier-resolution=node -r tsconfig-paths/register src/index.ts",
    "build": "rm -rf ./build/**/* && tsc -p tsconfig.json",
    "start": "node index.js",
    "start:dev": "node build/index.js",
    "format": "prettier",
    "prepare": "husky install"
  },
  "keywords": [
    "typescript",
    "fastify",
    "node.js",
    "docker"
  ],
  "dependencies": {
    "@fastify/accepts": "4.1.0",
    "@fastify/autoload": "5.6.0",
    "@fastify/basic-auth": "5.0.0",
    "@fastify/helmet": "10.1.0",
    "@fastify/rate-limit": "7.6.0",
    "@fastify/sensible": "5.2.0",
    "@fastify/type-provider-typebox": "2.4.0",
    "@sinclair/typebox": "0.25.16",
    "dotenv": "16.0.3",
    "fastify": "4.10.2",
    "fastify-plugin": "4.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@swc/core": "1.3.24",
    "@swc/helpers": "0.4.14",
    "@tsconfig/node18-strictest-esm": "^1.0.1",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "eslint": "8.30.0",
    "eslint-config-prettier": "8.5.0",
    "husky": "8.0.2",
    "lint-staged": "13.1.0",
    "nodemon": "2.0.20",
    "pino-pretty": "9.1.1",
    "prettier": "2.8.1",
    "regenerator-runtime": "0.13.11",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.1",
    "typescript": "4.9.4"
  }
}
